                          UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SELECT 
    CONVERT(INT, CONVERT(VARCHAR, OrderDate, 112)) AS DateInt ,
	OrderDate as fulldate ,
	DATEPART(quarter, OrderDate) as quartter,
	DATENAME(month, OrderDate) AS MonthName,

	CASE 
            WHEN DATEPART(month, OrderDate) BETWEEN 1 AND 6 THEN 'first_s'
            ELSE 'sec'
        END AS CalendarSemester
FROM 
    AdventureWorks2022.[Sales].[SalesOrderHeader]

UNION

SELECT 
    CONVERT(INT, CONVERT(VARCHAR, DueDate, 112)) AS DateInt ,
	DueDate as fulldate ,
	DATEPART(quarter,DueDate) as quartter,
	DATENAME(month, DueDate) AS MonthName,
	CASE 
            WHEN DATEPART(month, DueDate) BETWEEN 1 AND 6 THEN 'first_s'
            ELSE 'sec'
        END AS CalendarSemester


	
FROM 
    AdventureWorks2022.[Sales].[SalesOrderHeader]

UNION

SELECT 
    CONVERT(INT, CONVERT(VARCHAR, ShipDate, 112)) AS DateInt ,
	ShipDate as fulldate ,
	DATEPART(quarter, ShipDate) as quartter,
	DATENAME(month, ShipDate) AS MonthName,
	CASE 
            WHEN DATEPART(month, ShipDate) BETWEEN 1 AND 6 THEN 'first_s'
            ELSE 'sec'
        END AS CalendarSemester
FROM 
    AdventureWorks2022.[Sales].[SalesOrderHeader];</property>
